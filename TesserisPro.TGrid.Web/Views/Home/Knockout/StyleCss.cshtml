<pre class="brush: css">
.tgrid-root {
    position: relative;
}

.tgrid-table {
    border-collapse: collapse;
    width: 100%;
}

.tgrid-table thead tr th,
.tgrid-group-by-element,
.tgrid-goup-button .tgrid-menu li,
.tgrid-mobile-button .tgrid-menu li,
.tgrid-mobile-menu li {       
    border-right: 1px solid #BEBEBE;
    cursor: pointer;
    margin: 1px;
    padding: 2px;
    background: #EEE9E9; /* Old browsers */
    background: -moz-linear-gradient(top, #EEE9E9rgba(222,239,255,1) 0%, #DDDCDC 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(222,239,255,1)), color-stop(100%,#DDDCDC)); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top, #EEE9E9 0%,#DDDCDC 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top, #EEE9E9rgba(222,239,255,1) 0%,#DDDCDC 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top, #EEE9E9 0%,#DDDCDC 100%); /* IE10+ */
    background: linear-gradient(to bottom, #EEE9E9rgba(222,239,255,1) 0%,#DDDCDC 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#deefff', endColorstr='#98bede',GradientType=0 ); /* IE6-9 */
    color:grey;
}

.tgrid-mobile-header {
     border-bottom: 1px solid #BEBEBE;
}

.tgrid-table tbody tr td, .tgrid-table-group-header {
    overflow: hidden;
    line-height: 1.6em;
    vertical-align: middle;
    text-overflow: ellipsis;
    color: grey;
    padding: 2px .6em;
}

.tgrid-table-group-header {
    border-bottom: none !important;
    border-left: none !important;
    background-color: rgb(233, 234, 235) !important;
}

.tgrid-table-indent-cell {   
    background-color: rgb(233, 234, 235)  !important;
}

.tgrid-mobile-indent-div, .tgrid-table-indent-cell, .tgrid-mobile-group-indent-div{
    width: 20px;
}

.tgrid-table tbody tr.selected td {
    background-color: #dbdbde;
}

.tgrid-mobile-row, .tgrid-mobile-details, .tgrid-mobile-group-header {
    background-color: whitesmoke;
}

.tgrid-mobile-container .selected {
    background-color: #dbdbde;
}

.tgrid-tablebodycontainer {
    overflow-y: scroll;
    overflow-x: auto;
    height: auto;
    max-height:420px;
    border-right: 1px solid #BEBEBE;
    border-bottom: 1px solid #BEBEBE;
    border-left:1px solid #BEBEBE;
}

.tgrid-tableheadercontainer {
    border: 1px solid #BEBEBE;
    border-right: none;
    border-bottom: none;
}

.tgrid-pagination {
    padding-top: 3px;
    text-align: center;
}

.tgrid-mobile-group {
    border-left: 1px solid rgb(122,188,255);
    border-right: 1px solid rgb(122,188,255);
    border-bottom: 1px solid rgb(122,188,255);
}

.tgrid-page-current,
.tgrid-page-number {
    margin-left: 1px;
    margin-right: 1px;
    width: 17px;
    height: 18px;
    border: 1px solid #BEBEBE;
    display: inline-block;
}

.tgrid-page-number {
    color: black;
    cursor: pointer;
}

.tgrid-page-current {
    color: #A39A9A;
    cursor: default;
}

.tgrid-mobile-container.mobile {
    overflow:auto;
    max-height:488px;
    border-left: 1px solid #BEBEBE;
    border-right: 1px solid #BEBEBE;
    border-bottom: 1px solid #BEBEBE;
}

.tgrid-inline-block {
    display: inline-block;
}

/*Arrows*/
.tgrid-arrow-down {
    display: inline-block;
    width: 16px;
    height: 16px;
    background: url(images/arrow_up.png) no-repeat;
    margin-left: 5px;
}

.tgrid-arrow-up {
    display: inline-block;
    width: 16px;
    height: 16px;
    background: url(images/arrow_down.png) no-repeat;
    margin-left: 5px;
}

.tgrid-buisy-indicator {
    display: inline-block;
    position: absolute;
    left: 0px;
    right: 0px;
    top: 0px;
    bottom: 0px;
    background-color: rgba(122, 188, 255, 0.44);
}

.tgrid-scroll {
    display: inline-block;
    position: absolute;
    right: 1px;
    top: 30px;
    bottom: 36px;
    width:32px;
    overflow: scroll;
}

.tgrid-scroll div, .tgrid-mobile-group-header-container, .tgrid-mobile-group-indent-div {
    display: inline-block;
}

.tgrid-mobile-indent-div, .tgrid-mobile-div{
    display:inline-table;
    color: grey;
}

.details:hover, .details:focus{
    opacity: 0.9;
}
td a {
    text-decoration:none;
    cursor: pointer;
}
#main{
    width: 567px;
    height: auto;
    padding: 40px 7px 40px 7px;
    background:lightgray;
}

.tgrid-filter-popup {
    position: absolute;
    top: 16px;
    background: white;
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(222,239,255,1)), color-stop(100%,rgba(152,190,222,1)));
    border: 1px solid rgb(122,188,255);
    padding: 3px;
}

.tgrid-display-none{
    display: none;
}

.tgrid-filter-button, .tgrid-filter-indicator, .tgrid-filter-button-active{
    background: url(images/filter.png) no-repeat;
    display: inline-block;
    text-align: right;    
    height: 16px;
    width: 16px;
    position: absolute;
    top: 2px;
    right: 2px;
}
.tgrid-filter-button, .tgrid-filter-indicator {
    background: url(images/filter.png) no-repeat;
}
.tgrid-filter-button-active{
    background: url(images/filter_active.png) no-repeat;
}

.tgrid-group-button, .tgrid-group-indicator, .tgrid-group-button-active {
    background: url(images/group.png) no-repeat;
    display: inline-block;
    text-align: right;    
    height: 16px;
    width: 16px;
    position: absolute;
    top: 2px;
    right: 7px;
}
.tgrid-group-button-active{
    background: url(images/group_active.png) no-repeat;
}

.tgrid-sort-button,.tgrid-sort-button-asc, .tgrid-sort-button-desc, .tgrid-sort-indicator-asc, .tgrid-sort-indicator-desc {
    display: inline-block;
    text-align: right;    
    height: 16px;
    width: 16px;
    position: absolute;
    top: 2px;
    right: 44px;
}

.tgrid-sort-button {
    background: url(images/no-sort.png) no-repeat;
}

.tgrid-sort-indicator-asc, .tgrid-sort-button-asc{
    background: url(images/arrow_up.png) no-repeat;
}

 .tgrid-sort-indicator-desc, .tgrid-sort-button-desc{
    background: url(images/arrow_down.png) no-repeat;
}

th.tgrid-placeholder {
    min-width: 17px;
}

.tgrid-group-by-element {
    margin: 4px;
    padding: 2px;
    border: 1px solid #BEBEBE;
    height: 20px;
    min-width: 100px;
    float:left;
    position: relative;
    text-align: center;
}

.tgrid-group-by-element .tgrid-delete-button{   
    display: inline-block;
    width: 16px;
    height: 16px;
    background: url(images/delete.png) no-repeat;
    position: absolute;
    right: 2px;
    top: 4px;
    opacity: 0.8;
}


.tgrid-group-by-panel, .tgrid-mobile-group-header {    
    height: 33px;
    border-collapse: collapse;
    width: 100%;
}

.tgrid-goup-button, 
.tgrid-mobile-button  {
    display: inline-block;
    background-image: url(images/plus.png);
    background-repeat: no-repeat;
    margin-top: 4px;   
    margin-left: 3.5px;
    height: 24px;
    width: 24px;
    background-color: #EEE9E9;
    background-color: -moz-linear-gradient(top, #EEE9E9rgba(222,239,255,1) 0%, #DDDCDC 100%);
    background-color: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(222,239,255,1)), color-stop(100%,#DDDCDC));
    background-color: -webkit-linear-gradient(top, #EEE9E9 0%,#DDDCDC 100%);
    background-color: -o-linear-gradient(top, #EEE9E9rgba(222,239,255,1) 0%,#DDDCDC 100%);
    background-color: -ms-linear-gradient(top, #EEE9E9 0%,#DDDCDC 100%);
    background-color: linear-gradient(to bottom, #EEE9E9rgba(222,239,255,1) 0%,#DDDCDC 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#deefff', endColorstr='#98bede',GradientType=0 );   
    border: 1px solid grey;
    border-radius: 5px;
}

.tgrid-mobile-button .tgrid-header-cell-content {
    width: 80px;
}

.tgrid-mobile-button .tgrid-header-cell-buttons {
    min-width: 120px;
}

ul.tgrid-menu {
    position:absolute;
    padding: 0px;
    list-style-type: none;
    background: white; 
    border: 1px solid gray;
    margin: 32px 0px 0px 0px; 
    float: left;
    text-align: center;
    z-index:1;
}

ul.tgrid-menu li, ul.tgrid-mobile-menu li  {
    height: 22px;
    margin: 4px !important;
    min-width: 120px;
    position: relative;
    border: 1px solid #BEBEBE;
}

.tgrid-mobile-details {
    color: grey;
}

.hidden {
    display:none !important;
}

.visible{
    display:block !important;
    width: 100%;
}
.activeStyle
{
    background:green;
    color:green;
}

.tgrid-table tbody tr td input{
    display:none;
}

tr:nth-child(even), .tgrid-mobile-row:nth-child(even) {
  background-color: whitesmoke;
}


tr:nth-child(odd), .tgrid-mobile-row:nth-child(odd) {
  background-color: white;
}

/* Mobile */
@@media (max-width: 480px) {
    .desktop {
        display: none;
    }
	.tgrid-filter-popup {
		position: fixed;
		top: 16px;
		background: white;
		display: none;
		background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(222,239,255,1)), color-stop(100%,rgba(152,190,222,1)));
		border: 1px solid #BEBEBE;
		padding: 3px;
		z-index:1;
        width: 150px;
        height: 100px;
        top: 50%;
        left: 50%;
        margin-left: -105px;
        margin-top: -50px;
	}
	ul.tgrid-mobile-menu {
        position:fixed;
        padding: 0px;
        list-style-type: none;
        background: white; 
        border: 1px solid gray;
        margin: 32px 0px 0px 0px; 
        float: left;
        text-align: center;
        z-index:2;
        width: 200px;
        height: auto;
        left: 50%;
        top: 30%;
        margin-left: -105px;        
    }
}

/* Desktop */
@@media (min-width: 481px) {
    .mobile {
        display: none;
    }
}
</pre>
